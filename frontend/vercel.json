{
  "version": 2,
  "routes": [
    { "src": "^/$", "dest": "/client-order-form/index.html" },
    { "src": "^/order-form$", "dest": "/client-order-form/index.html" },
    { "src": "^/admin/login$", "dest": "/admin-dashboard/login.html" },
    { "src": "^/admin$", "dest": "/admin-dashboard/index.html" },
    { "src": "^/admin/$", "dest": "/admin-dashboard/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
