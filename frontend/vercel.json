{
  "rewrites": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "pedidos.axkan.art" }],
      "destination": "/client-order-form/index.html"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "pedidos.axkan.art" }],
      "destination": "/client-order-form/:path*"
    },
    { "source": "/styles.css", "destination": "/client-order-form/styles.css" },
    { "source": "/order-form.js", "destination": "/client-order-form/order-form.js" },
    { "source": "/logo.png", "destination": "/client-order-form/logo.png" },
    { "source": "/favicon.png", "destination": "/client-order-form/favicon.png" },
    { "source": "/tutorial.mp4", "destination": "/client-order-form/tutorial.mp4" },
    { "source": "/", "destination": "/landing/index.html" },
    { "source": "/landing", "destination": "/landing/index.html" },
    { "source": "/souvenirs", "destination": "/landing/souvenirs/index.html" },
    { "source": "/catalogo", "destination": "/landing/souvenirs/index.html" },
    { "source": "/catalogo/:destination/historia", "destination": "/landing/souvenirs/:destination/historia.html" },
    { "source": "/catalogo/assets/:path*", "destination": "/landing/souvenirs/assets/:path*" },
    { "source": "/catalogo/historia/:path*", "destination": "/landing/souvenirs/historia/:path*" },
    { "source": "/souvenirs/:destination/historia", "destination": "/landing/souvenirs/:destination/historia.html" },
    { "source": "/souvenirs/assets/:path*", "destination": "/landing/souvenirs/assets/:path*" },
    { "source": "/souvenirs/historia/:path*", "destination": "/landing/souvenirs/historia/:path*" },
    { "source": "/souvenirs/:slug", "destination": "/landing/souvenirs/:slug.html" },
    { "source": "/productos/:slug", "destination": "/landing/productos/:slug.html" },
    { "source": "/productos", "destination": "/landing/productos/imanes-mdf.html" },
    { "source": "/sitemap.xml", "destination": "/landing/sitemap.xml" },
    { "source": "/robots.txt", "destination": "/landing/robots.txt" },
    { "source": "/assets/:path*", "destination": "/landing/assets/:path*" },
    { "source": "/fonts/:path*", "destination": "/landing/fonts/:path*" },
    { "source": "/globe-section.js", "destination": "/landing/globe-section.js" },
    { "source": "/destination-images.json", "destination": "/landing/destination-images.json" },
    { "source": "/mexico-states.json", "destination": "/landing/mexico-states.json" },
    { "source": "/pedidos", "destination": "/client-order-form/index.html" },
    { "source": "/pedidos/:path*", "destination": "/client-order-form/:path*" },
    { "source": "/order-form", "destination": "/client-order-form/index.html" },
    { "source": "/order-form/:path*", "destination": "/client-order-form/:path*" },
    { "source": "/cuestionario", "destination": "/lead-form/index.html" },
    { "source": "/cuestionario/:path*", "destination": "/lead-form/:path*" },
    { "source": "/midiseño", "destination": "/configurador/index.html" },
    { "source": "/midiseño/:path*", "destination": "/configurador/:path*" },
    { "source": "/midise%C3%B1o", "destination": "/configurador/index.html" },
    { "source": "/midise%C3%B1o/:path*", "destination": "/configurador/:path*" },
    { "source": "/midiseno", "destination": "/configurador/index.html" },
    { "source": "/midiseno/:path*", "destination": "/configurador/:path*" },
    { "source": "/envio", "destination": "/shipping-form/index.html" },
    { "source": "/envio/:path*", "destination": "/shipping-form/:path*" },
    { "source": "/seguimiento", "destination": "/order-tracking/index.html" },
    { "source": "/seguimiento/:path*", "destination": "/order-tracking/:path*" },
    { "source": "/admin/login", "destination": "/admin-dashboard/login.html" },
    { "source": "/admin", "destination": "/admin-dashboard/index.html" },
    { "source": "/admin/", "destination": "/admin-dashboard/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
